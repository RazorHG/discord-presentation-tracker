{
    "compilerOptions": {
      "target": "ES6",
      "module": "Node16",
      "rootDir": "./src",
      "outDir": "./dist",
      "removeComments": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "strictNullChecks": true,
      "skipLibCheck": true,
      "moduleResolution": "node16"
    }
  }